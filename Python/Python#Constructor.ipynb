    "class ComplexNumber:\n",
    "    def __init__(self, r = 0, i = 0):\n",
    "        self.real = r\n",
    "        self.imag = i\n",
    "        \n",
    "    def getData(self):\n",
    "        print(\"{0}+{1}j\".format(self.real, self.imag))\n",
    "    \n",
    "if __name__ == '__main__':\n",
    "    c = ComplexNumber(5,6)\n",
    "    c.getData()"
